<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project1.mapper.UserMapper">

<select id="findAll"  resultType="com.projectv1.domain.User">
 select * from users
</select>

<select id="findById" parameterType="int" resultType="com.projectv1.domain.User">
 select * from users (name, email) values (#{name}, #{email})
</select>

<select id="insert" parameterType="com.project1.domain.User">
	insert into users(name, email) values (#{name},#{email})
</select>

<select id="update" parameterType="com.project1.domain.User">
	update users set name= #{name}, email= #{email} where id = #{id}
</select>

<delete id="delete" parameterType="com.project1.domain.User">
	delete from users where id = #{id}
</delete>

</mapper>